<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zengqiang.composestudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeStudy">
        <activity
            android:name=".module.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeStudy.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.ui.TextActivity" />
        <activity android:name=".module.UiActivity" />
        <activity android:name=".module.ui.ButtonActivity" />
        <activity android:name=".module.ui.OutLineButtonActivity" />
        <activity android:name=".module.ui.TextButtonActivity" />
        <activity android:name=".module.ui.TextFieldActivity" />
        <activity android:name=".module.ui.OutlinedTextFieldActivity" />
        <activity android:name=".module.ui.IconActivity" />
    </application>

</manifest>